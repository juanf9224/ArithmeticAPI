{
  "name": "loan-pro-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "18.16.1",
    "npm": "9.5.1"
  },
  "scripts": {
    "preinstall": "npx check-engine",
    "ts-node": "cross-env NODE_OPTIONS=\"--loader=ts-node/esm --no-warnings --experimental-specifier-resolution=node\" node",
    "start": "yarn ts-node src/index.ts",
    "predevelop": "yarn db:migrate && yarn db:seed",
    "dev": "nodemon",
    "lint": "eslint src",
    "prettier": "./node_modules/.bin/prettier --write \"{src,knex}/**/*.ts\"",
    "pretest": "cross-env NODE_ENV=test yarn db:rollback:all && cross-env NODE_ENV=test yarn db:migrate",
    "pretest:CI": "cross-env NODE_ENV=test yarn db:migrate",
    "test": "jest --env=node",
    "test:CI": "cross-env NODE_OPTIONS=\"--max-old-space-size=3072\" jest --maxWorkers=2",
    "watch": "jest --watchAll",
    "knex": "yarn ts-node node_modules/knex/bin/cli.js",
    "db:migrate": "yarn knex migrate:latest",
    "db:migrate:make": "yarn knex migrate:make",
    "db:rollback": "yarn knex migrate:rollback",
    "db:rollback:all": "yarn knex migrate:rollback --all",
    "db:reset": "yarn db:rollback:all && yarn db:setup",
    "db:seed": "yarn knex seed:run",
    "db:setup": "yarn db:migrate && yarn db:seed"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/swagger-ui-express": "^4.1.3",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fetch-mock": "^9.11.0",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "jwt-decode": "^3.1.2",
    "knex": "^2.5.1",
    "morgan": "^1.10.0",
    "objection": "^3.0.5",
    "sqlite3": "^5.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.4",
    "@types/rosie": "^0.0.42",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "6.6.6",
    "jest": "^28.1.3",
    "nodemon": "^3.0.1",
    "prettier": "^2.7.1",
    "rosie": "^2.1.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
